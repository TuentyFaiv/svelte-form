{
	"name": "@faivform/builtin",
	"description": "Built-in Svelte forms using @tuentyfaiv/svelte-form",
	"version": "0.0.2",
	"type": "module",
	"scripts": {
		"dev:web": "vite dev",
		"build:web": "vite build && pnpm build",
		"preview:web": "vite preview",
		"sync": "svelte-kit sync",
		"dev": "pnpm sync && svelte-package -w",
		"build": "pnpm sync && svelte-package && publint",
		"test": "pnpm test:integration && pnpm test:unit",
		"check": "pnpm sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"keywords": [
		"Svelte",
		"Forms",
		"UI",
		"Builtin Forms"
	],
	"author": "Tonalli LÃ³pez <contact@tuentyfaiv.com>",
	"license": "MIT",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"peerDependencies": {
		"@emotion/css": "^11.11.2",
		"@tuentyfaiv/svelte-form": "workspace:^",
		"svelte": "^4.0.0",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@emotion/css": "^11.11.2",
		"@playwright/test": "^1.36.0",
		"@repo/eslint-config": "workspace:*",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@sveltejs/package": "^2.2.0",
		"@tuentyfaiv/svelte-form": "workspace:^",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.44.0",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-module-utils": "^2.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-scope": "^7.2.0",
		"eslint-visitor-keys": "^3.4.1",
		"publint": "^0.1.16",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.5.1",
		"vitest": "^0.33.0",
		"yup": "^1.2.0"
	},
	"engines": {
		"npm": ">=8.0.0",
		"pnpm": ">=8.0.0",
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TuentyFaiv/svelte-form.git"
	},
	"bugs": {
		"url": "https://github.com/TuentyFaiv/svelte-form/issues"
	},
	"homepage": "https://github.com/TuentyFaiv/svelte-form/packages/builtin#readme",
	"directories": {
		"test": "tests"
	}
}