{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@icons/*": [
        "./src/assets/icons/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@sharing/atoms": [
        "./src/ui/sharing/atoms/index.ts"
      ],
      "@sharing/organisms": [
        "./src/ui/sharing/organisms/index.ts"
      ],
      "@home/organisms": [
        "./src/ui/home/organisms/index.ts"
      ],
      "@builtin/organisms": [
        "./src/ui/builtin/organisms/index.ts"
      ],
      "@sharing/organisms/*": [
        "./src/ui/sharing/organisms/*"
      ],
      "@sharing/atoms/*": [
        "./src/ui/sharing/atoms/*"
      ],
      "@home/organisms/*": [
        "./src/ui/home/organisms/*"
      ],
      "@builtin/organisms/*": [
        "./src/ui/builtin/organisms/*"
      ],
    }
  }
}